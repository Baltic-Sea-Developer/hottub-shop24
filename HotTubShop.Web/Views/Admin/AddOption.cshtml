@model AdminOptionEditViewModel
@{
    ViewData["Title"] = "Option anlegen";
}

<h1 class="h3 mb-3">Option anlegen</h1>
@if (TempData["AdminError"] is string adminError && !string.IsNullOrWhiteSpace(adminError))
